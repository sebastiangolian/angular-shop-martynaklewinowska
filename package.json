{
  "name": "angular-shop-mk",
  "version": "0.15.0",
  "browserslist": [
    "> 5%"
  ],
  "scripts": {
    "start": "ng serve --o --port=4203",
    "start-test": "ng serve -o --c=testing  --port=4400",
    "build": "ng build",
    "build-ghpages": "ng build --prod --base-href=/angular-shop-mk/ --outputPath=dist/angular-shop-mk",
    "build-prod": "ng build --prod",
    "deploy-test": "node --trace-warnings deploy.js environment=test version=%npm_package_version%",
    "deploy-prod": "node deploy.js environment=prod version=%npm_package_version%",
    "preghpages": "ng build --c=ghpages --base-href=/angular-shop-mk/ --outputPath=dist/angular-shop-mk",
    "ghpages": "gh-pages -d dist/angular-shop-mk/",
    "test": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e --port=4201 --webdriverUpdate=true",
    "clear": "rmdir /q /s node_modules",
    "analyze": "ng build --prod --stats-json && webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.3",
    "@angular/common": "^10.2.3",
    "@angular/compiler": "^10.2.3",
    "@angular/core": "^10.2.3",
    "@angular/forms": "^10.2.3",
    "@angular/platform-browser": "^10.2.3",
    "@angular/platform-browser-dynamic": "^10.2.3",
    "@angular/router": "^10.2.3",
    "@auth0/angular-jwt": "^5.0.2",
    "file-saver": "^2.0.2",
    "ngx-bootstrap": "^6.1.0",
    "ngx-cookie-service": "^10.1.1",
    "ngx-mask": "^10.0.1",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.0",
    "@angular/cli": "^10.2.0",
    "@angular/compiler-cli": "^10.2.3",
    "@angular/language-service": "^10.2.3",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^12.12.50",
    "ftp-deploy": "^2.3.8",
    "codelyzer": "^6.0.1",
    "gh-pages": "^3.1.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.4",
    "protractor": "~7.0.0",
    "ts-node": "~8.6.2",
    "tslint": "^6.1.0",
    "typescript": "^3.9.7"
  }
}
